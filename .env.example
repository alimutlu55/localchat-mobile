# LocalChat Mobile Environment Variables
# Copy this file to .env and fill in your values
# Get these from Google Cloud Console: https://console.cloud.google.com/apis/credentials

# Google OAuth Client IDs (all required for production)
# Values starting with EXPO_PUBLIC_ are automatically available in your React code
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:8080/api/v1
EXPO_PUBLIC_WS_URL=ws://localhost:8080/ws

# AdMob App IDs (Used at build-time in app.config.js)
# Do NOT use EXPO_PUBLIC_ prefix for these to keep them build-time only
ADMOB_ANDROID_APP_ID=ca-app-pub-3940256099942544~3347511713
ADMOB_IOS_APP_ID=ca-app-pub-3940256099942544~1458002511
